
<h2>Todo</h2>
<div class="todo-column"><h2>Future</h2>
  <% toDos.future.forEach(function(todo) { %>
    <% include ./todoItem %>
  <% }); %>
</div>
<div class="todo-column"><h2>Present</h2>
  <% toDos.current.forEach(function(todo) { %>
    <% include ./todoItem %>
  <% }); %>
</div>
<div class="todo-column"><h2>Done</h2>
  <% toDos.done.forEach(function(todo) { %>
    <% include ./todoItem %>
  <% }); %>
</div>
